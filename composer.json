{
    "name": "kilobyteno/laravel-user-guest-like",
    "description": "A Laravel package to allow guests and users to like models",
    "keywords": [
        "laravel",
        "like",
        "guest",
        "user",
        "kilobyteno",
        "kilobyte"
    ],
    "homepage": "https://github.com/kilobyteno/laravel-user-guest-like",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel S. Billing",
            "email": "daniel@kilobyte.no",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.10|^6.0|^7.0",
        "orchestra/testbench": "^6.22|^7.0|^8.0",
        "pestphp/pest": "^1.23",
        "pestphp/pest-plugin-laravel": "^1.3",
        "phpunit/phpunit": "^9.5",
        "spatie/laravel-ray": "^1.32"
    },
    "autoload": {
        "psr-4": {
            "Kilobyteno\\LaravelUserGuestLike\\": "src",
            "Kilobyteno\\LaravelUserGuestLike\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kilobyteno\\LaravelUserGuestLike\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kilobyteno\\LaravelUserGuestLike\\LaravelUserGuestLikeServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
